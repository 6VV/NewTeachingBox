<?xml version="1.0" encoding="utf-8"?>
<Root>
  <!--Parameter类型包括：Position,Dynamic,Overlap,Integer,Double,Bool,String,RefSys,ToolSys-->
  <!--请务必保证此文件内容的正确性，程序不对此文件正确性进行检查，若出现错误，则可能导致意料之外的情况-->
  <!--需要发送指令码的宏命令（如MOVL、MOVC）id建议从较大值（如1000）开始，以便与其它命令（如IF、FOR）区别，id值不可相同-->
  <Category name="MOVEMENT">
    <Type name="MOVEMENT">
      <Macro name = "MOVL" id="2000">
        <Parameter>Position</Parameter>
        <Parameter>Dynamic</Parameter>
        <Parameter>Overlap</Parameter>
        <Text>MOVL *,*,*</Text>
      </Macro>
      <Macro name = "MOVC" id="2004">
        <Parameter>Position</Parameter>
        <Parameter>Position</Parameter>
        <Parameter>Dynamic</Parameter>
        <Parameter>Overlap</Parameter>
        <Text>MOVC *,*,*,*</Text>
      </Macro>
    </Type>
  </Category>
  <Category name="FUNCTION">
    <Type name="MATH">
      <Macro name="SIN">
        <Text>SIN *</Text>
      </Macro>
      <Macro name="COS">
        <Text>COS *</Text>
      </Macro>
      <Macro name="TAN">
        <Text>TAN *</Text>
      </Macro>
      <Macro name="SQRT">
        <Text>SQRT *</Text>
      </Macro>
    </Type>
    <Type name="STRUCTURE">
      <Macro name="GOSUB">
        <Text>GOSUB *</Text>
      </Macro>
      <Macro name="FOR">
        <Text>FOR *&#x000A;NEXT</Text>
      </Macro>
      <Macro name="IF">
        <Text>IF *&#x000A;ENDIF</Text>
      </Macro>
      <Macro name="ELSEIF">
        <Text>ELSE *</Text>
      </Macro>
      <Macro name="SWITCH">
        <Text>SWITCH *&#x000A;DEFAULT&#x000A;BREAK&#x000A;ENDSWITCH</Text>
      </Macro>
      <Macro name="CASE">
        <Text>CASE *&#x000A;BREAK</Text>
      </Macro>
      <Macro name="BREAK">
        <Text>BREAK</Text>
      </Macro>
    </Type>
    <Type name="PROGRAM">
      <Macro name="ABORT">
        <Text>ABORT</Text>
      </Macro>
    </Type>
    <Type name="FUNCTION">
      <Macro name="WAIT">
        <Text>WAIT *</Text>
      </Macro>
    </Type>
    <Type name="OTHERS">
      <Macro name="SET_TOOLFRAME">
        <Parameter>ToolSys</Parameter>
        <Text>SET_TOOLFRAME *</Text>
      </Macro>
      <Macro name="SET_USERFRAME">
        <Text>SET_USERFRAME *</Text>
      </Macro>
    </Type>
  </Category>
  <Category name="IO">
    <Type name="DIGITAL">
      <Macro name="DIN"></Macro>
      <Macro name="DOUT"></Macro>
    </Type>
    <Type name="ANALOG">
      <Macro name="AIN"></Macro>
      <Macro name="AOUT"></Macro>
    </Type>
  </Category>
  <Category name="WELD">
    <Type name="ARC_WELDING">
      <Macro name="ARC_ON"></Macro>
      <Macro name="ARC_OFF"></Macro>
      <Macro name="SET_ARC"></Macro>
    </Type>
    <Type name="SPOT_WELDING">
      <Macro name="SPOT_ON"></Macro>
      <Macro name="SPOT_OFF"></Macro>
      <Macro name="SET_SPOT"></Macro>
    </Type>
  </Category>
</Root>
